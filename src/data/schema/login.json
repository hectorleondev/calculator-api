{
   "$schema":"http://json-schema.org/draft-04/schema#",
   "title":"login",
   "type":"object",
   "properties":{
      "username":{"type":"string", "format": "email", "pattern": "^\\S+@\\S+\\.\\S+$"},
      "password":{"type":"string", "minLength": 8}
   },
   "required":[
      "username",
      "password"
   ]
}